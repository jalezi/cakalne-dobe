{
  "name": "cakalne-dobe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "drizzle:local:push": "drizzle-kit push:sqlite --config=./drizzle/config/local.ts",
    "drizzle:prod:push": "drizzle-kit push:sqlite --config=./drizzle/config/prod.ts",
    "drizzle:local:studio": "drizzle-kit studio --config=./drizzle/config/local.ts --port 4000",
    "drizzle:prod:studio": "drizzle-kit studio --config=./drizzle/config/prod.ts --port 4001",
    "db:local:seed": "node -r dotenv/config  -r esbuild-register mock-data/seed-db.ts dotenv_config_path=.env.development.local",
    "db:prod:seed": "node -r dotenv/config  -r esbuild-register mock-data/seed-db.ts dotenv_config_path=.env.production.local",
    "turso:dev": "turso dev",
    "turso:dev:file": "turso dev --db-file dev.db"
  },
  "dependencies": {
    "@libsql/client": "^0.6.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/match-sorter-utils": "^8.15.1",
    "@tanstack/react-table": "^8.16.0",
    "@tanstack/table-core": "^8.16.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.8",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "lucide-react": "^0.368.0",
    "next": "14.2.1",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.0",
    "vaul": "^0.9.0",
    "zod": "^3.22.4"
